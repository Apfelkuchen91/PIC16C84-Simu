; ZÃ¤hlt bei steigender Flanke (PIN RA - Bit 0) das Register COUNTER hoch

	DEVICE    16F84
		CONFIG    RBPU_ON
		CONFIG    WDT_ON
		CONFIG    ANSEL
		CONFIG    ANSELH

		
COUNTER			EQU		15H
CURRENT			EQU		16H
TEMP_BIT		EQU		17H
TEMP_CURR		EQU		18H
		
	org		0

	MOVLW 0CCH        ; Register W = 0
	MOVWF TEMP_CURR  ; 18H == TEMP

	end